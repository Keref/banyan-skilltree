<p id="notice"><%= notice %></p>

<h1>My Courses</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Course</th>

      <th>Level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skills.each do |skill| %>
			<% if skill.node.nodetype == "graph" %>
				<tr>
					<td><%= link_to skill.node.name, skill_path(skill.node.id) %></td>

					<td><%= skill.lvl %></td>


					<td><%= link_to 'Unenroll', skill, method: :delete, data: { confirm: 'All progress will be lost, Are you sure?' } %></td>
				</tr>
			<% end %>
    <% end %>
  </tbody>
</table>

