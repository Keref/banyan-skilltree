<%= will_paginate %>

<ul class="icons">
  <% @icons.each do |icon| %>
		<i><img src="/assets<%= icon %>" width="32" height="32"/></i>
	<% end %>
</ul>

<%= will_paginate %>

<script>
	$(".paginate a").each(function(i,link){
		var hr = link.href;
		link.href = '#';
		link.attr('target',hr);
	});
</script>
